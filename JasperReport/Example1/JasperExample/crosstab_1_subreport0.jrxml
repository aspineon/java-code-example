	<queryString><![CDATA[SELECT  --SIZE_TABLE.NAME NAME,
--        SUM(COMMODITY.QUANTITY) QUANTITY
        ARTICLE.NAME ARTICLE_NAME,
        SIZE_TABLE.NAME SIZE_TABLE_NAME,
        SUM(COMMODITY.QUANTITY) QUANTITY
FROM COMMODITY
INNER JOIN ARTICLE ON ARTICLE.KOD=COMMODITY.KOD
--    INNER JOIN SUPPLIER ON SUPPLIER.KOD=ARTICLE.KOD_SUPPLIER
--    INNER JOIN KIND ON KIND.KOD=ARTICLE.KOD_KIND
    INNER JOIN SIZE_TABLE ON SIZE_TABLE.KOD=ARTICLE.KOD_SIZE_TABLE
WHERE ARTICLE.NAME=$P{ARTICLE_NAME}
-- RUPPER(ARTICLE.NAME) LIKE '4502-0100/10'
--AND SUPPLIER.KOD=1
--AND KIND.KOD=43
AND COMMODITY.KOD_POINT_SOURCE=0
GROUP BY ARTICLE.NAME,
         SIZE_TABLE.NAME]]></queryString>

